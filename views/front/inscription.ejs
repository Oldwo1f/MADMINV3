<body class="stretched inscription"  ng-controller="appController">
	
	<!-- <link rel="stylesheet" href="demos/travel/css/datepicker.css" type="text/css" /> -->
	<link rel="stylesheet" href="styles/front/components/timepicker.css" type="text/css" />
	<link rel="stylesheet" href="styles/front/components/bs-select.css" type="text/css" />
